{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["ESNext", "WebWorker"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationMap": true,
    "outFile": "dist/index.d.ts"
  },
  "exclude": ["dist"]
}
