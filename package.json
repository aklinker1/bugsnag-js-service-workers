{
  "name": "bugsnag-js-windowless",
  "description": "Bugsnag client for JS contexts without the 'window' global",
  "version": "0.1.0",
  "files": [
    "lib",
    "dist"
  ],
  "repository": "https://github.com/aklinker1/bugsnag-js-windowless",
  "author": {
    "name": "Aaron Klinker",
    "email": "aaronklinker1@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "bugsnag",
    "web-worker",
    "service-worker",
    "web",
    "service",
    "worker",
    "chrome",
    "browser",
    "extension",
    "chrome-extension",
    "web-extension",
    "browser-extension",
    "no",
    "window"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "compile": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "vite dev",
    "build": "vite build",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@bugsnag/core": "^7.16.1",
    "@types/chance": "^1.1.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "chance": "^1.1.8",
    "detect-browser": "^5.3.0",
    "dotenv": "^16.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  },
  "peerDependencies": {
    "@bugsnag/core": "^7.16.1"
  }
}
